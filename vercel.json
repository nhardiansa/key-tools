{
  "rewrites": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "feedback.domain.com"
        }
      ],
      "destination": "/feedback/$1"
    },
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "admin.domain.com"
        }
      ],
      "destination": "/admin/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/$1"
    }
  ]
}
